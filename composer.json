{
  "name": "liangtao/v2dmim-core",
  "type": "library",
  "description": "PHP core class library",
  "keywords": [
    "php",
    "v2dmim",
    "v2dmim-core"
  ],
  "homepage": "https://github.com/liangtao-top/v2dmim-core",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "TaoGe",
      "email": "liangtao.gz@foxmail.com",
      "role": "Developer",
      "homepage": "https://github.com/liangtao-top"
    }
  ],
  "require": {
    "php": ">=8.0.12",
    "ext-zlib": "*",
    "ext-mbstring": "*",
    "liangtao/etcd-php": "^1.0.0"
  },
  "require-dev": {
    "swoole/ide-helper": "@dev",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "V2dmIM\\Core\\": "src/"
    },
    "files": [
      "src/constant.php",
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "V2dmIM\\Tests\\Core\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  ]
}
